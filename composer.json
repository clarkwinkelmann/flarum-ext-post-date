{
    "name": "clarkwinkelmann/flarum-ext-post-date",
    "description": "Let mods update the create and edit time of posts",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.9"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\PostDate\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Post Date",
            "icon": {
                "name": "far fa-clock",
                "backgroundColor": "#000",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": ""
        }
    }
}
